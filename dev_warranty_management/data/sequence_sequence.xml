<odoo>
    <record id="seq_warranty" model="ir.sequence">
        <field name="name">warranty.warranty</field>
        <field name="code">warranty.warranty</field>
        <field name="prefix">WAR/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <record id="seq_ticket_number" model="ir.sequence">
        <field name="name">helpdesk.ticket.waranty</field>
        <field name="code">helpdesk.ticket.waranty</field>
        <field name="prefix">TKT/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
